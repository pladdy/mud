#alias use_corpse {
  !hp;
  !cease;
  #if {$gp1 == $gp1_max} {!kt};
  di;
  !hp;
  !get all;
}

#alias f10 {use_corpse}

#alias f12 {
  #if {$map[current_room] == 13692} {
    #loop 2 8 i {!donate all from bag $i};
  } {
    #loop 2 8 i {!sell all from bag $i};
  }
}

#alias di {
  !remove shield;
  !perform dirge;
  !wear shield;
}

#alias kt {
  #if {%1} {!karma tap %1} {!karma tap 20};
}
