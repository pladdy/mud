#alias ammo {
  run juggers_ammo;
  #if {"%1" == "proj"} {!vbuy projectile 2 1 10000};
  #if {"%1" == "miss"} {!vbuy missile 6 1 10000};
}

#alias f10 {
  !hp;
  !fdeener;
  !hp;
  !get all;
}

#alias f12 {
  #switch {$map[current_room]} {
    #case {$map[current_room] == 11438 || $map[current_room] == 11437} {
      #loop 2 8 i {!donate all from bag $i};
    }
    #default { #loop 2 8 i {!sell all from bag $i} };
  };
}

#alias rede {
  !unwrap;
  !fdeener;
}
