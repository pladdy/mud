#alias kcommands_before_corpse {
  !study spellbook;
  #if {$gp1 < 90} {!cast spell tap on corpse};
}

#alias f10 {
  !hp;
  #nop #if {$gp1 < 90} {!cast spell tap on corpse};
  !cast corpse blast;
  !get all;
  !hp;
}

#alias rebl {
  !unwrap corpse;
  #if {$gp1 < 90} {!cast spell tap on corpse};
  !cast corpse blast;
}
