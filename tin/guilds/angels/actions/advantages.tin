#action {^HO: %1/%2 HI: %3/%4 LO: %5/%6 LI: %7/%8 G: %9} {
  #var gxp {%9};
  #replace {gxp} {%} {};

  show_status_bar;

  #if { @length{%1} > 0 } {
    #var advantages[ho] %1;
    #var advantages[hi] %3;
    #var advantages[lo] %5;
    #var advantages[li] %7;

    #var adv_sort {};
    sort_advantages;
    attempt_swing;
    attempt_presage;
    attempt_disengage;
  };
}
