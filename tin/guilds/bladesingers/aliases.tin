#alias f10 {
  use_corpse;
}

#alias pi {
  !remove %1;
  !pinscribe %1 on %2;
}

#alias pp {
  !remove %1;
  !pprepare %1;
}

#alias resl {
  !unwrap;
  use_corpse
}

#alias resls {
  #if {$corpses > 0} {
    #if {$corpses < 5} {#var to_use {$corpses}} {#var to_use {5}};
    #loop 1 $to_use $loop {
      resl
    };
    #unvar to_use;
  }
}

#alias use_corpse {
  !hp;
  !salute;
  !bury;
  !hp;
}


