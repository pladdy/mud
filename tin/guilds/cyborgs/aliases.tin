#alias cos {
  !unwield $wielded_weapon;
  !configure strategy %0;
  !wield $wielded_weapon;
}

#alias f12 {
  #loop 2 12 i {!sell all from bag $i};
}

#alias redc {
  !unwrap corpse;
  !decompose;
}

#alias reet {
  !unwrap corpse;
  !eat corpse;
}

#alias use_corpse {
  #if {$gp1 == $gp1_max} {!eat corpse};
  #else {!decompose};
  !hp;
}
