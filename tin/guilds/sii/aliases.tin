#alias use_corpse {
  !hp;
  !envelop corpse;
  !collect dna from corpse;
  !hp;
  !get all;
}

#alias f10 {use_corpse}

#alias f12 {
  #if {$map[current_room] == 22754} {
    #loop 2 8 i {
      !get all from bag $i;
      !feed all;
    };
  }
}

#alias reet {
  !unwrap corpse;
  !eat corpse;
}

#alias reinf {
  !unwrap corpse 1;
  !infest;
}

#alias reenc {
  !unwrap corpse;
  !envelop corpse;
  !collect dna from corpse;
}
