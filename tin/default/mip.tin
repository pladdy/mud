#nop Link;
#nop Dev page: http://gameaxle.com/;
#nop MIP doc: http://gameaxle.com/PortalMIP9.pdf;

#class mip kill;
#class mip open;

#var MIP_SEC_CODE 10675;
#var MIP_VERSION 9.000;

#alias send_3klient {
  #send 3klient $MIP_SEC_CODE~Portal$MIP_VERSION;
  #SEND 3klient LINEFEED on;
}

#action {^#K%{$MIP_SEC_CODE}{\d{3}}{[A-Z]{3}}} {
  #showme <170>MIP Match;
  #showme The 0 var: %0;
  #showme The 1 var: %1;
  #showme The 2 var: %2;
  #showme The 3 var: %3;
}

#class mip close;
