#nop If using Cyberpope amulet, this keeps it on;
#action {^The force field surrounding you dies out.$} {
  #if {$enemy > 0} {pbm};
}

#action {^You wear the %1.} {
  #send !keep %1;
}

#action {^You wield %1.$} {
  #send !keep %1;
}

#action {Skipping: %1 will bind on pickup.$} {
  log.warn Bindy item: %1;
  sound_beeps;
}
