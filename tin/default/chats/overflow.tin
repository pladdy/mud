#class {chats_overflow} {kill};
#class {chats_overflow} {open};

#action {^%!{[> ]*}$} {
  #class chats_overflow kill;
};

#nop  Ignore android crits;
#action {^           a critical shot!} {
  #nop;
}

#action {^{\s{8,20}}\S%2$} {
  log.debug chat overflow detected;
  #list chats add {"%0"};
  chats_refresh;
};

#class {chats_overflow} {close};
